@article{rogers2012igw,
doi = {10.1088/2041-8205/758/1/L6},
url = {https://dx.doi.org/10.1088/2041-8205/758/1/L6},
year = {2012},
month = {sep},
publisher = {The American Astronomical Society},
volume = {758},
number = {1},
pages = {L6},
author = {T. M. Rogers and D. N. C. Lin and H. H. B. Lau},
title = {INTERNAL GRAVITY WAVES MODULATE THE APPARENT MISALIGNMENT OF EXOPLANETS AROUND HOT STARS},
journal = {The Astrophysical Journal Letters},
abstract = {We propose that the observed misalignment between extrasolar planets
    and their hot host stars can be explained by angular momentum transport
        within the host star. Observations have shown that this misalignment is
        preferentially around hot stars, which have convective cores and
        extended radiative envelopes. This situation is amenable to substantial
        angular momentum transport by internal gravity waves (IGW) generated at
        the convective-radiative interface. Here, we present numerical
        simulations of this process and show that IGW can modulate the surface
        rotation of the star. With these two-dimensional simulations, we show
        that IGW could explain the retrograde orbits observed in systems such as
        HAT-P-6 and HAT-P-7. However, extension to high-obliquity objects will
        await future three-dimensional simulations. We note that these results
        also imply that individual massive stars should show temporal variations
        in their v sin i measurements.}
}

